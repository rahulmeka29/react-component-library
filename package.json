{
  "version": "0.13.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "react-component-library",
  "author": "Rahul Meka",
  "module": "dist/react-component-library.esm.js",
  "size-limit": [
    {
      "path": "dist/react-component-library.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-component-library.esm.js",
      "limit": "10 KB"
    }
  ],
  "scripts": {
    "dev": "run-p bundle generate-css storybook",
    "format": "prettier --write README.md \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "bundle": "tsup src/index.ts --format esm,cjs --dts --external react --watch",
    "generate-css": "postcss src/styles/tailwind.css -o public/main.css --watch"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@size-limit/preset-small-lib": "^8.1.0",
    "@storybook/addon-essentials": "^6.5.14",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.5.14",
    "@storybook/addons": "^6.5.14",
    "@storybook/react": "^6.5.14",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "babel-loader": "^9.1.0",
    "husky": "^8.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "size-limit": "^8.1.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3",
    "@storybook/addon-a11y": "6.5.9",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/testing-library": "^0.0.13",
    "autoprefixer": "^10.4.7",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "postcss-cli": "^9.1.0",
    "postcss-focus-visible": "^6.0.4",
    "postcss-import": "^14.1.0",
    "tailwindcss": "^3.1.4",
    "tsup": "^6.1.2"
  },
  "overrides": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-refresh": "0.13.0"
  }
}
